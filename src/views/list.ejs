<!DOCTYPE html>
<html lang="pt-br">
<%- include('partials/head.ejs')%>
<body>
    <h1><%= title %></h1>

    <%- include('partials/navbar')%>

    

    <ul>
        <% users.forEach((user) =>{%>
        <li>
            <%= user.name%> - <%= user.email%> - Idade: <%= user.age%> -
            <a href="/edit?id=<%= user._id%>">[Editar]</a> - 
            <a href="/remove/<%= user._id%>">[Excluir]</a>
        </li>    
        <%})%>
    </ul>
            
    <%- include('partials/foot.ejs')%>
</body>
</html>